<h1>Welcome</h1>
<p>Current user id: <%= current_user.id %>. <span>Current user username: <%= current_user.username %></span></p>

<div class="row">
  <div class="col-4">
    <h3>Conversation List</h3>
    <ul>
      <% @conversations.each_with_index do |conversation, index| %>
        <li>
          <span><%= index %></span> - <%= conversation.status %>
        </li>
      <% end %>
    </ul>
  </div>
  
  <div class="col-8">
  
  </div>
</div>
